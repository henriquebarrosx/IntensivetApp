{
    "expo": {
        "main": "index.js",
        "version": "1.0.0",
        "slug": "intensivet",
        "orientation": "portrait",
        "icon": "./assets/images/logo-icon.png",
        "name": "Intensivet Saúde Digital",
        "jsEngine": "hermes",
        "runtimeVersion": "1.0.0",
        "splash": {
            "resizeMode": "contain",
            "backgroundColor": "#ffffff",
            "image": "./assets/images/splash.png"
        },
        "notification": {
            "icon": "./assets/images/logo-icon.png"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "updates": {
            "fallbackToCacheTimeout": 0,
            "url": "https://u.expo.dev/369e150f-abfc-4e24-9ee5-88a2db8bd8a3"
        },
        "extra": {
            "eas": {
                "projectId": "369e150f-abfc-4e24-9ee5-88a2db8bd8a3"
            }
        },
        "plugins": [
            [
                "expo-document-picker",
                {
                    "appleTeamId": "SK8DM654PP"
                }
            ],
            [
                "expo-image-picker",
                {
                    "photosPermission": "Permitir Intensivet Saúde Digital acessar sua câmera & fotos da galeria."
                }
            ],
            [
                "expo-camera",
                {
                    "cameraPermission": "Permitir Intensivet Saúde Digital acessar sua câmera."
                }
            ],
            [
                "expo-media-library",
                {
                    "photosPermission": "Permitir Intensivet Saúde Digital acessar sua galeria.",
                    "savePhotosPermission": "Permitir Intensivet Saúde Digital salvar fotos.",
                    "isAccessMediaLocationEnabled": true
                }
            ]
        ],
        "android": {
            "useNextNotificationsApi": true,
            "permissions": [
                "android.permission.CAMERA",
                "android.permission.RECEIVE_BOOT_COMPLETED",
                "android.permission.RECORD_AUDIO",
                "android.permission.READ_EXTERNAL_STORAGE",
                "android.permission.WRITE_EXTERNAL_STORAGE"
            ],
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/logo-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "versionCode": 1
        },
        "ios": {
            "jsEngine": "hermes",
            "buildNumber": "3.1.7",
            "supportsTablet": true,
            "usesIcloudStorage": true,
            "entitlements": {
                "com.apple.developer.networking.wifi-info": true
            },
            "infoPlist": {
                "NSCameraUsageDescription": "Nós precisamos disso para que você possa compartilhar arquivos com outros veterinários"
            }
        }
    }
}